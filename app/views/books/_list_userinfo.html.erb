<h2>User info</h2>
<%= image_tag list.get_profile_image(100,100) %>
<table class="table">
  
  <tbody>
    <tr>
      <th>name</th>
      <th><%= list.name %></th> <!--ログイン指定いるUserのnameを入れる-->
    </tr>
    <tr>
      <th>introduction</th>
      <th><%= list.introduction %></th> <!--ログイン指定いるUserのnameを入れる-->
    </tr>
  </tbody>
</table>

<!-- user/edit遷移ボタン-->
<div class="row">
    <%= link_to edit_user_path(list.id), class: "btn btn-outline-secondary btn-block" do %>
    <i class="fas fa-user-cog"></i>
    <% end %>
</div>
